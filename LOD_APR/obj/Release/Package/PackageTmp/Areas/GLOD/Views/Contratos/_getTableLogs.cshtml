@model IEnumerable<Galena.Areas.ASP.Models.ASP_log>
<table class="table table-condensed table-hover" id="tablaLogs" name="tablaLogs">
    <thead>
        <tr>
            <th>Fecha Logs</th>
            <th>Usuario</th>
            <th>Acción</th>
            <th>Campo</th>
            <th>Valor Anterior</th>
            <th>Valor Actual</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => item.FechaLog)</td>
                <td>@Html.DisplayFor(modelItem => item.NombUser)</td>
                <td>@Html.DisplayFor(modelItem => item.Accion)</td>
                <td>@Html.DisplayFor(modelItem => item.Campo)</td>
                <td>@Html.DisplayFor(modelItem => item.ValorAnterior)</td>
                <td>@Html.DisplayFor(modelItem => item.ValorActualizado)</td>
            </tr>
        }
    </tbody>
</table>
